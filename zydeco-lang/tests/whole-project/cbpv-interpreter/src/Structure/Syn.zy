use Std_next/ Std/ String/ String end

data SynComp end

pub data SynVal where
  | +Var(String)
  | +True()
  | +False()
  | +ThunkV(SynComp)
end

pub data SynComp where
  | +If(SynVal, SynComp, SynComp)
  | +Force(SynVal)
  | +Lam(String, SynComp)
  | +App(SynComp, SynVal)
  | +Return(SynVal)
  | +Bind(SynComp, String, SynComp)
end